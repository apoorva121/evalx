# Langfuse Configuration
LANGFUSE_PUBLIC_KEY=your-public-key-here
LANGFUSE_SECRET_KEY=your-secret-key-here
LANGFUSE_HOST=https://cloud.langfuse.com

# OpenAI Configuration
OPENAI_API_KEY=your-openai-key-here

# Python Path - Add evalx_sdk to import path for development
# REMOVE THIS IN PRODUCTION - use proper package installation instead
export PYTHONPATH="${PYTHONPATH}:${PWD}/../evalx_sdk"

# EvalX Service Configuration (for run tracking)
EVALX_SERVICE_URL=http://localhost:8000
EVALX_BEARER_TOKEN=your-bearer-token-here
EVALX_ENV=local
EVALX_MODE=local
